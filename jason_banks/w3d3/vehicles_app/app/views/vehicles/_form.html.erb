<%= form_for @vehicle do |v| %>
  <%= v.label :type_of_vehicle %>:
  <%= v.select "type_of_vehicle", options_for_select(@type_of_vehicle_array) %><br />
  <%= v.label :make %>:
  <%= v.text_field :make, 'v.make' if v.make %><br />
  <%= v.label :model %>:
  <%= v.text_field :model %><br />
  <%= v.label :country %>:
  <%= v.text_field :country %><br />
  <%= submit_tag("Submit")%>
<% end %>