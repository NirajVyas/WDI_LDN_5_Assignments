these are the stocks currently in your portfolio

<a href='/portfolios/<%= @portfolio.id %>/stocks/new'>new stock</a>


<ul>
  <% @portfolio.stocks.each do |stock| %>
    <li>
      <%= stock.name %>
      <%= stock.portfolio.name if stock.portfolio %>
      <a href='/stocks/<%=stock.portfolio.id%>/edit'>edit</a>
      <a href='/stocks/<%=stock.portfolio.id%>/delete'>delete</a>
    </li>
  <% end %>
</ul>