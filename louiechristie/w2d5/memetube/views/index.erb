<% @videos.each do |video| %>

    <p>
      <a href="/show/<%=video['id'] %>"> 
        <%=video['id'] %> <strong><%=video['title'] %></strong>
      </a> 
      <%= video['description'] %> <%= video['url'] %> <%= video['genre'] %>
    </p>

    <iframe width="560" height="315" src="//www.youtube.com/embed/<%= parse_youtube(video['url']) %> " frameborder="0" allowfullscreen></iframe>


<% end %>