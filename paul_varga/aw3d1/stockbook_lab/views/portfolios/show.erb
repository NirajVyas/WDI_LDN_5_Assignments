<% @portfolio.stocks.each do |stock| %>

  <h3>Symbol: <%= stock.symbol %></h3>
  <p>Holding: <%= stock.holding %></p>

  <p>Name: <%= stock.name %></p>
  <p>Buying Price: <%= stock.buying_price %></p>
  <p>Current Value: <%= stock.value %></p>
  <p>Current Price: <%= stock.current_price %></p>

  <p>Portfolio: <%= stock.portfolio.name if stock.portfolio %></p>

  <a href='/stocks/<%= stock.id %>/edit'>edit stocks</a>
  <a href='/stocks/<%= stock.id %>/delete'>delete stocks</a>


<% end %>