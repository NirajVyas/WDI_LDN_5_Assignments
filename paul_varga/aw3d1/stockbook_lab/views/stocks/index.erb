<a href='/stocks/new'>new stock</a>

<ul>
<% @stocks.each do |stock| %>

<li>
    <%= stock.name %>
    <%= stock.portfolio.name if stock.portfolio %>
    Name: <%= YahooFinance::get_standard_quotes(stock.symbol)[stock.symbol].name %>
    Price: <%= YahooFinance::get_standard_quotes(stock.symbol)[stock.symbol].lastTrade %>
    <a href='/stocks/<%= stock.id %>/edit'>edit</a>
    <a href='/stocks/<%= stock.id %>/delete'>delete</a>
</li>

<% end %>
</ul>