<h2> </h2>

<section class="show">

    <div class="tv">
      <img src="old_tv_cropped.png" />
    </div>

</section>




<section class="library">

    <% i = "" %>
    <% @users_videos.each do |video| %>
         <div class="video<%= i %>">
            <img class="video_spine" src='vhs_spine.jpg' />
            <h3 class="video_title"> <%= video["title"] %> : <%= video["genre"] %></h3>
            <p class="description">  Title: <%= video["title"] %> <br />Description: <%= video["description"] %> <br /> Genre: <%= video["genre"] %> </p>    
            <iframe width="560" height="315" src="//www.youtube.com/embed/<%= video["url"] %>" frameborder="0" allowfullscreen></iframe>
            <a class="delete" href="delete/<%= video["id"] %>"a>Delete</a>
            <a class= "edit" href="edit/<%= video["id"] %>"a>Edit</a>
            <img class="play_button" src='play_button.png' />
         </div>
    <% end %>
    
</section>









