<!DOCTYPE html>
<html>
<head>
  <title>MoviesHomeworkRails</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
		<% if current_user %>
			<%= link_to "Home", root_url %>
			<%= link_to 'New User', new_user_path %>
			<%= link_to "Manage Movies", movies_path %>
			<%= link_to "Manage Formats", formats_path %>
			<%= link_to "Logout", session_path(0), method: :delete %>
		<% else %>
			<%= link_to "Home", root_url %>
		<% end %>



<%= yield %>

</body>
</html>
